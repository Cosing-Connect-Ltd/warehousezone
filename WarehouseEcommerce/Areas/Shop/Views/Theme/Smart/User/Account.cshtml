@{
    ViewBag.Title = "Account";
}

<div class="row about bg-white custom-max-width m-auto pb-0">
            @Html.Partial("_AccountSideBar")
            <div class="col-lg-9 col-md-12 bg-white login my-account">
                <div class="about-cnt">
                    <div class="content-wrapper select">
                        <h2 class="text-uppercase">My Account</h2>
                        <hr />
                        <h4>Contact Information</h4>
                        <hr />
                        <form method="post" action="#">
                            <div class="">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Enter your First Name">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Enter your Last Name">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="email" class="form-control input-md inputNew" size="18" placeholder="Enter your E-mail ID">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Enter your Mobile Number">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="button-submit">
                                            <button type="Submit" name="submitNewsletter" class="btn btn-block account-save-btn">Save Details</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <h4>Address Information</h4>
                        <hr />
                        <form method="post" action="#">
                            <div class="">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Address Line 1">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Address Line 2">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Land Mark">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control input-md inputNew" size="18" placeholder="Zip Code">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <select name="city">
                                                <option>City 1</option>
                                                <option>City 2</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <select name="State">
                                                <option>State 1</option>
                                                <option>State 2</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <select name="Country">
                                                <option>Country 1</option>
                                                <option>Country 2</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="button-submit">
                                            <button type="Submit" name="submitNewsletter" class="btn btn-block account-save-btn">Save Details</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <h4>Change Password</h4>
                        <hr />
                        <form method="post" action="#">
                            <div class="">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="password" class="form-control input-md inputNew" size="18" placeholder="Enter your old password">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="password" class="form-control input-md inputNew" size="18" placeholder="Enter your new password">
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="password" class="form-control input-md inputNew" size="18" placeholder="Re-Enter your new password">
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="button-submit">
                                            <button type="Submit" name="submitNewsletter" class="btn btn-block account-save-btn">Reset Password</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
</div>