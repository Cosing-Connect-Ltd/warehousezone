  <!-- footer section -->
@model Ganedata.Core.Services.caTenantWebsites
<footer class="nb_footer">
    <div class="container">
        <div class="nb_footersec_inner">
            <a href="" class="nb_foot_logo">
                <img src="~/Content/Theme/University/app/images/logo.svg" />
            </a>
            @if (!string.IsNullOrEmpty(Model.FacebookUrl) || !string.IsNullOrEmpty(Model.InstaGramUrl) || !string.IsNullOrEmpty(Model.LinkedInUrl) || !string.IsNullOrEmpty(Model.TwitterUrl))
            {
                <div class="followUsWrap">
                    <h5>Follow Us On</h5>
                    <ul>
                        @if (!string.IsNullOrEmpty(Model.FacebookUrl))
                        {
                            <li>

                                <a href=""><i class="ico fb icon-facebook-logo"></i></a>
                            </li>
                        }
                        @if (!string.IsNullOrEmpty(Model.TwitterUrl))
                        {
                            <li>
                                <a href=""><i class="ico tw icon-twitter-logo"></i></a>
                            </li>
                        }
                        @if (!string.IsNullOrEmpty(Model.LinkedInUrl))
                        {
                            <li>
                                <a href=""><i class="ico ln icon-linkedin-logo"></i></a>
                            </li>
                        }
                        @if (!string.IsNullOrEmpty(Model.InstaGramUrl))
                        {
                            <li>
                                <a href=""><i class="ico ig icon-instagram-logo"></i></a>
                            </li>
                        }
                    </ul>
                </div>
            }
            <div class="nb_footnav_wrap">
                <ul>

                    <li><a href="">Sitemap</a></li>
                    <li><a href="">FAQ</a></li>
                    @if (!string.IsNullOrEmpty(Model.WebsiteContactPhone)
                        )
                    {
                        <li><a href="">Contact</a></li>
                    }
                    @if (!string.IsNullOrEmpty(Model.WebsiteContactAddress))
                    {
                        <li><a href="">Delivery</a></li>
                    }
                </ul>
            </div>
            <ul class="nb_foot_payment_opt_lst">
                <li>
                    <a href=""><span class="card1"></span></a>
                </li>
                <li>
                    <a href=""><span class="card2"></span></a>
                </li>
                <li>
                    <a href=""><span class="card3"></span></a>
                </li>
                <li>
                    <a href=""><span class="card4"></span></a>
                </li>
            </ul>
            <hr />
            <div class="nb_footcontact_wrap">
                <p>
                    Official Northumbria University merchandise ordering system
                    powered by Creative Emporium on behalf of Northumbria
                    University<br />
                    Disclaimer | Privacy | Copyright Notice | Accessibility | Freedom
                    of Information | Charitable Status | Royal Charter Number :
                    XR11395<br />
                    Infinity Incorporated Ltd is the legal owner of Northumbria
                    University e-shop. Company number 3582519
                </p>
            </div>
        </div>
    </div>
</footer>
<!-- footer section -->
