@model List<Ganedata.Core.Entities.Domain.WebsiteWishListItem>
@{
    ViewBag.Title = "WishList";
}


<section class="nb_sec nb_prdlisting_pgsec">
    <div class="container">
        <div class="breadCrumbtopsec">
            <div class="nb_bread_crumbwrap">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="/"><i class="icon-home"></i></a>
                        </li>
                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                        <li class="breadcrumb-item"><a href="#">WishList</a></li>
                    </ol>
                </nav>
            </div>
        </div>
        <div class="nb_sec">
            <div class="nb_header_style1">
                <h3>WishList</h3>
            </div>

            <div class="row">

                <div class="nb_basket_wrap">
                    <table class="basketTable">
                        <thead>
                            <tr>
                                <th>Product</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        <div class="prdimgsec">
                                            <span class="prdimg">
                                                <img src="@(!string.IsNullOrEmpty(item.ProductMaster.DefaultImage)? ViewBag.BaseFilePath + item.ProductMaster.DefaultImage : ViewBag.BaseFilePath + "/UploadedFiles/Products/no_image.gif")" />
                                            </span>
                                            <span class="prdname">@(item.ProductMaster?.Name)</span>
                                        </div>
                                    </td>
                                    <td>
                                        <div>
                                            <a href="javascript:;" class="btn btn-success" onclick="AddToCart(@item.ProductMaster.ProductId)">Add to Basket</a> <br /><br />
                                            <a href="javascript:;" class="btn btn-danger">Remove</a>
                                        </div>
                                    </td>
                                   
                                </tr>
                            }


                        </tbody>
                    </table>

                    <div class="coupen_codewrap">
                    </div>


                </div>


            </div>
        </div>
    </div>
</section>

