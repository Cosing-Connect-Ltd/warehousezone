<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://warehousezone.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>warehousezone.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>WarehouseZone</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$WarehouseZone</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ApplicationContext" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="Ganedata.Core.Entities.Domain.ApplicationContext, WMS" MigrationConfiguration="WMS.Migrations.Configuration, WMS" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="WMS_ERROR_LOG" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=l43zbffach.database.windows.net,1433;Initial Catalog=WAREHOUSEZONE_ERROR_LOG;Integrated Security=False;User ID=wmsadmin;Password=admin123!;Connect Timeout=0" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\WMS_ERROR_LOG_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=l43zbffach.database.windows.net,1433;Initial Catalog=WAREHOUSEZONE_ERROR_LOG;Integrated Security=False;User Id=wmsadmin;Password=admin123!;Timeout=0;MultipleActiveResultSets=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ApplicationContext-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)WMS_ERROR_LOG-Web.config Connection String" />
  </ItemGroup>
</Project>