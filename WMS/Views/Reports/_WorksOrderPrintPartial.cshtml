@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "PurchaseOrderPrint";
    settings.CallbackRouteValues = new { Controller = "Reports", Action = "WorksOrderPrintPartial" };
    settings.ExportRouteValues = new { Controller = "Reports", Action = "WorksOrderPrintPartialExport" };
    settings.Report = (XtraReport)Model;
    settings.ToolbarMode = DocumentViewerToolbarMode.Ribbon;
}).GetHtml()