@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "SalesOrderPrint";
    settings.CallbackRouteValues = new { Controller = "Reports", Action = "SalesOrderPrintPartial" };
    settings.ExportRouteValues = new { Controller = "Reports", Action = "SalesOrderPrintPartialExport" };
    settings.Report = (XtraReport)Model;
    settings.ToolbarMode = DocumentViewerToolbarMode.Ribbon;
}).GetHtml()