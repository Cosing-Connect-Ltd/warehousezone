@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "Works order- Expensive properties Report";
    settings.CallbackRouteValues = new { Controller = "Reports", Action = "_WorksOrderExpensivePropertiesReportPartial" };
    settings.ExportRouteValues = new { Controller = "Reports", Action = "_WorksOrderExpensivePropertiesReportExport" };
	settings.Report = (XtraReport)Model;
    
    settings.ToolbarMode = DocumentViewerToolbarMode.Ribbon;
    settings.StylesSplitter.SidePaneMinWidth = System.Web.UI.WebControls.Unit.Pixel(300);
     
}).GetHtml()