@{
    var grid = Html.DevExpress().GridView(settings =>
    {
        settings.Name = "RDetail_" + ViewData["ReceivePOId"];
        settings.SettingsDetail.MasterGridName = "_RcvList";
        settings.CallbackRouteValues = new { Controller = "RecevePO", Action = "RDetail", ReceivePOId = ViewData["ReceivePOId"] };
        settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);

        settings.KeyFieldName = "ReceivePOId";

       
        settings.Columns.Add("GTIN");
        settings.Columns.Add("QtyReceived");
       
    });
}
@grid.Bind(Model).GetHtml()

