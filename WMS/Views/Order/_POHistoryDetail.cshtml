
@{
    var grid = Html.DevExpress().GridView(settings =>
    {
        settings.Name = "_POHistoryDetail_" + ViewData["POID"];
        settings.SettingsDetail.MasterGridName = "History";
        settings.CallbackRouteValues = new { Controller = "PO", Action = "_POHistoryDetail", POID = ViewData["POID"] };
        settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);

        settings.KeyFieldName = "Qty";



        settings.Columns.Add("GTIN");
        settings.Columns.Add("Warehouse");
        settings.Columns.Add("Qty");
        settings.Columns.Add("QtyReceived");

    });
}
@grid.Bind(Model).GetHtml()

