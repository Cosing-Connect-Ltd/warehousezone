@model IEnumerable<Ganedata.Core.Entities.Domain.ProductMaster>




<table class="table table-hover table-bordered tsort">
   <thead>
     <tr>
		 <th>
              @Html.Label("Select")

           </th>
        <th>
            @Html.DisplayNameFor(model => model.SKUCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShortDesc)
        </th>
        
    </tr>
	</thead>
   <tbody>

@if (Model != null) { 
 foreach (var item in Model) {
    <tr>
        <td>
            <input type="radio" name="id" class="selkey" value="@item.ProductId">
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SKUCode)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.ShortDesc)
        </td>
            
    </tr>
}
}
</tbody>
</table>




